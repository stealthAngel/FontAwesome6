<Window
    x:Class="DesktopClient.Framework.FontAwesome.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:svgnet="clr-namespace:DesktopClient.Framework.FontAwesome.svgnet"
    Title="MainWindow"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <Grid>
        <svgnet:SvgAwesome
            Icon="{Binding Path=Aap}"
            PrimaryColor="Green"
            PrimaryOpacity="1"
            SecondaryColor="blue"
            SecondaryOpacity="1"
            SwapOpacity="false" />
    </Grid>
</Window>
